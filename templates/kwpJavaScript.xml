<templateSet group="kwpJavaScript">
  <template name="3lc" value="//&#10;// $END$&#10;//" description="Three line comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="clog" value="console.log(&quot;$END$&quot;);" description="console.log()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="f" value="function $name$($params$) {$END$}$term$" description="JavaScript function" toReformat="false" toShortenFQNames="true">
    <variable name="term" expression="" defaultValue="&quot;;&quot;" alwaysStopAt="true" />
    <variable name="name" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="params" expression="" defaultValue="&quot;param1, param2&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="for" value="for ($counter$ = 0; $counter$ &lt; $limit$; ++$counter$) {$END$&#10;}" description="for loop" toReformat="false" toShortenFQNames="true">
    <variable name="counter" expression="" defaultValue="&quot;itemIndex&quot;" alwaysStopAt="true" />
    <variable name="limit" expression="" defaultValue="&quot;aContainer.length&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="forown" value="for (prop in $instanceName$) {&#10;    if ($instanceName$.hasOwnProperty(prop)) {&#10;        $END$&#10;    }&#10;}&#10;" toReformat="false" toShortenFQNames="true">
    <variable name="instanceName" expression="" defaultValue="&quot;objectVar&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="global" value="/* global $END$ */" description="global comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="if" value="if ($END$) {&#10;}" description="if statement" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="iife" value="(function () {&#10;    $END$&#10;})();" description="Immediately Invoked Function Expression" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="jsdctor" value="/**&#10; * Constructs a new $className$.&#10; *&#10; * @class&#10; * @classdesc classDesc&#10; *&#10; * @param {typeA} paramNameA - paramADesc&#10; * @param {typeB} paramNameB - paramBDesc&#10; */" description="JSDoc for constructor" toReformat="false" toShortenFQNames="true">
    <variable name="className" expression="" defaultValue="&quot;MyClass&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="jsdfunc" value="/**&#10; * $END$&#10; * @param {Type} paramName - ParameterDescription&#10; * @returns {Type} ReturnDescription&#10; */" description="JSDoc for a function" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="jsdprop" value="/**&#10; * $propDescription$&#10; * @name $className$#$propName$&#10; * @type $type$&#10; * @default $defaultValue$&#10; */" description="JSDoc for a property" toReformat="false" toShortenFQNames="true">
    <variable name="propDescription" expression="" defaultValue="&quot;Property description&quot;" alwaysStopAt="true" />
    <variable name="className" expression="" defaultValue="&quot;ClassName&quot;" alwaysStopAt="true" />
    <variable name="propName" expression="" defaultValue="&quot;propName&quot;" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="&quot;type&quot;" alwaysStopAt="true" />
    <variable name="defaultValue" expression="" defaultValue="&quot;defaultValue&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="kwpclass" value="var $ConstructorName$ = (function () {&#10;&#10;    /**&#10;     * Constructs a new $ConstructorName$.&#10;     *&#10;     * @class&#10;     * @classdesc Describe_the_class_here&#10;     * @param {typeA} paramNameA - paramADesc&#10;     * @param {typeB} paramNameB - paramBDesc&#10;     */&#10;    function $ConstructorName$() {&#10;&#10;        // Data members&#10;        // todo: Define data members.&#10;        // this.name = 'Kevin';&#10;    }&#10;&#10;    // Member functions&#10;    // todo: Define methods.&#10;    // $ConstructorName$.prototype.myMethod = function () { };&#10;&#10;    return $ConstructorName$;&#10;})();$END$" description="A JavaScript class (ES5)" toReformat="false" toShortenFQNames="true">
    <variable name="ConstructorName" expression="" defaultValue="&quot;ConstructorName&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sep" value="////////////////////////////////////////////////////////////////////////////////" description="Separator comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="str" value="'$str$'$END$" description="A string" toReformat="false" toShortenFQNames="true">
    <variable name="str" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="strict" value="&quot;use strict&quot;;$END$" description="&quot;use strict&quot; statement" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="jsdmethod" value="/**&#10; * $END$&#10; * @method&#10; * @param {Type} paramName - ParameterDescription&#10; * @returns {Type} ReturnDescription&#10; */" description="JSDoc for a method" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="arrow" value="($params$) =&gt; {$END$}" description="insert an arrow function" toReformat="false" toShortenFQNames="true">
    <variable name="params" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cdump" value="console.log(&quot;----- $varName$ -----&quot;);&#10;console.dir($varName$);$END$" description="console.log a variable" toReformat="false" toShortenFQNames="true">
    <variable name="varName" expression="" defaultValue="&quot;var&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cdir" value="console.dir($varName$);$END$" description="console.dir() a variable" toReformat="false" toShortenFQNames="true">
    <variable name="varName" expression="" defaultValue="&quot;var&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="gothere" value="console.log(&quot;----- $message$ -----&quot;);$END$" description="console.log() a message" toReformat="false" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="&quot;GOT HERE&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="then" value="&#10;.then($END$)" description="then" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="catch" value="&#10;.catch($END$)" description="catch" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="she" value="#!$ENV$ $NODE$" description="shebang line" toReformat="false" toShortenFQNames="false">
    <variable name="ENV" expression="" defaultValue="&quot;/usr/bin/env&quot;" alwaysStopAt="false" />
    <variable name="NODE" expression="" defaultValue="&quot;node&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="jsdrest" value="/**&#10; * $method$ $route$&#10; * $description$&#10; */" description="JSDoc-style comment for a RESTful endpoint" toReformat="false" toShortenFQNames="true">
    <variable name="method" expression="" defaultValue="&quot;GET&quot;" alwaysStopAt="true" />
    <variable name="route" expression="" defaultValue="&quot;/api/v2/store&quot;" alwaysStopAt="true" />
    <variable name="description" expression="" defaultValue="&quot;description&quot;" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>